{
  "family": "ecogram-backend-task",
  "taskRoleArn": "arn:aws:iam::113204170412:role/ecsTaskRole",
  "executionRoleArn": "arn:aws:iam::113204170412:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "ecogram-backend",
      "image": "113204170412.dkr.ecr.us-east-1.amazonaws.com/ecogram-backend:latest",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "LANG",
          "value": "en_US.UTF-8"
        },
        {
          "name": "LC_ALL",
          "value": "en_US.UTF-8"
        },
        {
          "name": "PYTHONIOENCODING",
          "value": "utf-8"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "MONGO_URI",
          "valueFrom": "arn:aws:ssm:us-east-1:113204170412:parameter/ecogram/MONGO_URI"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:ssm:us-east-1:113204170412:parameter/ecogram/JWT_SECRET"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ecogram-backend",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}
